---
layout: none
---
[
    {% for trees in site.posts %}
    {
      "title"    : "{{ trees.title | escape }}",
      "common-name"     : "{{ trees.common-name }}",
      "url"      : "{{ site.baseurl }}{{ trees.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for trees in site.pinus %}
    {
      "title"    : "{{ trees.title | escape }}",
      "common-name"     : "{{ trees.common-name }}",
      "url"      : "{{ site.baseurl }}{{ trees.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for page in site.pages %}
    {
      "title"    : "{{ page.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ page.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}

]
---
layout: none
genuses:
  - abies
  - pinus
---
[
  {% for post in site.abies %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %},
  {% for post in site.cathaya %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %},
  {% for post in site.cedrus %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %},
  {% for post in site.keteleeria %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %},
  {% for post in site.pinus %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  {% for post in site.tsuga %}
    {
      "title"    : "{{ post.title | escape }}",
      "common-name" : "{{ post.common-name }}",
      "species-name"     : "{{ post.species-name }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]